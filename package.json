{
	"name": "papyrus",
	"displayName": "Papyrus Code",
	"description": "A package for working with Mark Hanna's Papyrus scripting language.",
	"publisher": "Scrivener07",
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#1e415e",
		"theme": "dark"
	},
	"license": "SEE LICENSE IN LICENSE.md",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.9.0"
	},
	"homepage": "https://github.com/Scrivener07/Papyrus-Code/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/Scrivener07/Papyrus-Code.git"
	},
	"bugs": {
		"url": "https://github.com/Scrivener07/Papyrus-Code/issues"
	},
	"keywords": [
		"papyrus",
		"fallout-4"
	],
	"categories": [
		"Languages",
		"Snippets",
		"Other"
	],
	"main": "./out/src/main",
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"mocha": "^2.3.3",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	},
	"activationEvents": [
		"onLanguage:papyrus",
		"onLanguage:papyrus-assembly",
		"onLanguage:papyrus-compiled",
		"onLanguage:papyrus-project",
		"onCommand:extension.papyrusHello",
		"onCommand:extension.papyrusCount"
	],
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Papyrus Settings",
			"properties": {
				"papyrus.installDirectory": {
					"type": "string",
					"default": "MyInstallDirectory",
					"description": "Specifies the folder path containing `Fallout4.exe`."
				},
				"papyrus.outputDirectory": {
					"type": "string",
					"default": "MyOutputDirectory",
					"description": "Specifies the folder path to output compiled papyrus files."
				}
			}
		},
		"commands": [
			{
				"command": "extension.papyrusHello",
				"title": "Papyrus - Hello World!"
			},
			{
				"command": "extension.papyrusCount",
				"title": "Papyrus - Count Selection"
			},
			{
				"command": "papyrus.showPreview",
				"title": "Papyrus - Show Preview"
			},
			{
				"command": "papyrus.showPreviewToSide",
				"title": "Papyrus - Show Preview To Side"
			}
		],
		"menus": {
			"editor/title": [
				{
					"when": "resourceLangId == papyrus",
					"command": "papyrus.showPreview",
					"alt": "papyrus.showPreviewToSide",
					"group": "navigation"
				}
			]
		},
		"languages": [
			{
				"id": "papyrus",
				"aliases": [
					"Papyrus",
					"papyrus"
				],
				"extensions": [
					".psc"
				],
				"configuration": "./papyrus-configuration.json"
			},
			{
				"id": "papyrus-assembly",
				"aliases": [
					"Papyrus Assembly",
					"papyrus-assembly"
				],
				"extensions": [
					".pas"
				],
				"configuration": "./papyrus-assembly-configuration.json"
			},
			{
				"id": "papyrus-compiled",
				"aliases": [
					"Papyrus Compiled",
					"papyrus-compiled"
				],
				"extensions": [
					".pex"
				],
				"configuration": "./papyrus-compiled-configuration.json"
			},
			{
				"id": "papyrus-project",
				"aliases": [
					"Papyrus Project",
					"papyrus-project"
				],
				"extensions": [
					".ppj"
				],
				"configuration": "./papyrus-project-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "papyrus",
				"scopeName": "source.papyrus",
				"path": "./syntaxes/papyrus.tmLanguage"
			},
			{
				"language": "papyrus-assembly",
				"scopeName": "source.papyrus-assembly",
				"path": "./syntaxes/papyrus-assembly.tmLanguage"
			},
			{
				"language": "papyrus-compiled",
				"scopeName": "source.papyrus-compiled",
				"path": "./syntaxes/papyrus-compiled.tmLanguage"
			},
			{
				"language": "papyrus-project",
				"scopeName": "text.xml.papyrus-project",
				"path": "./syntaxes/papyrus-project.json"
			}
		],
