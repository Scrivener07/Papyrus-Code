{
    "name": "papyrus",
    "displayName": "Papyrus Code",
    "description": "A package for working with Mark Hanna's Papyrus scripting language.",
	"publisher": "Scrivener07",
    "icon": "images/icon.png",
	"galleryBanner":
	{
		"color": "#1e415e",
		"theme": "dark"
	},
    "license": "SEE LICENSE IN LICENSE.md",
    "version": "0.0.1",
	"engines":
    {
        "vscode": "^1.9.0"
    },
    "homepage": "https://github.com/Scrivener07/Papyrus-Code/blob/master/README.md",
    "repository":
    {
		"type": "git",
		"url": "https://github.com/Scrivener07/Papyrus-Code.git"
	},
	"bugs":
	{
		"url": "https://github.com/Scrivener07/Papyrus-Code/issues"
	},
	"keywords":
	[
		"papyrus",
		"fallout-4"
	],
    "categories":
    [
        "Languages",
        "Snippets"
    ],
    "contributes":
    {
        "languages":
        [
            {
                "id": "papyrus",
                "aliases": ["Papyrus", "papyrus"],
                "extensions": [".psc"],
                "configuration": "./language-configuration.json"
            },
			{
				"id": "papyrus-assembly",
				"aliases": ["Papyrus Assembly", "papyrus-assembly"],
				"extensions": [".pas"]
			},
			{
				"id": "papyrus-compiled",
				"aliases": ["Papyrus Compiled", "papyrus-compiled"],
				"extensions": [".pex"]
			}
        ],
        "grammars":
        [
            {
                "language": "papyrus",
                "scopeName": "source.papyrus",
                "path": "./syntaxes/Papyrus.tmLanguage"
            },
			{
				"language": "papyrus-assembly",
				"scopeName": "source.papyrus-assembly",
				"path": "./syntaxes/Papyrus-Assembly.tmLanguage"
			},
			{
				"language": "papyrus-compiled",
				"scopeName": "source.papyrus-compiled",
				"path": "./syntaxes/Papyrus-Compiled.tmLanguage"
			}
        ],
        "snippets":
        [
            {
                "language": "papyrus",
                "path": "./snippets/language.json"
            }
        ]
    }
}
